{
	"name": "Lazarus Resurrection Theme",
	"type": "dark",
	"colors": {
		// Core Editor
		"editor.background": "#182026",
		"editor.foreground": "#88dbfd",
		// "editorCursor.foreground": "#5E97EC",
		// "editor.lineHighlightBackground": "#1A2236",
		// "editor.selectionBackground": "#2D3A6A80",
		// "editor.selectionHighlightBackground": "#3A4A7D60",
		// UI Elements
		// "activityBar.background": "#182026",
		// "activityBar.foreground": "#93C5FD",
		"sideBar.background": "#182026",
		// "statusBar.background": "#182026",
		// "statusBar.foreground": "#88dbfd",
		// Syntax Highlights
		// "editor.findMatchBackground": "#F59E0B",
		// "editor.findMatchHighlightBackground": "#FBBF2460",
		// "editorBracketMatch.background": "#3A4A7D60",
		// HTML/CSS Specific
		// "editor.wordHighlightBackground": "#5E97EC30",
		// "editor.wordHighlightStrongBackground": "#818CF830"
	},
	"tokenColors": [
		{
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#546e7a",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"foreground": "#c9e797",
			}
		},
		{
			"scope": [
				"variable",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#edffff"
			}
		},
		{
			"scope": [
				"string",
				"punctuation.definition.string"
			],
			"settings": {
				"foreground": "#c9e797"
			}
		},
		{
			"scope": [
				"entity.name.tag",
				"entity.name.tag.html"
			],
			"settings": {
				"foreground": "#ef7078"
			}
		},
		{
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#c28ee4"
			}
		},
		{
			"scope": [
				"support.class",
				"support.type"
			],
			"settings": {
				"foreground": "#f9cb6b"
			}
		},
		{
			"scope": [
				"meta.selector.css",
				"entity.name.tag.css"
			],
			"settings": {
				"foreground": "#5EEAD4"
			}
		},
		{
			"scope": [
				"meta.property-name.css"
			],
			"settings": {
				"foreground": "#93C5FD"
			}
		},
		{
			"scope": [
				"meta.property-value.css"
			],
			"settings": {
				"foreground": "#86EFAC"
			}
		},
		{
			"scope": [
				"invalid"
			],
			"settings": {
				"foreground": "#FB7185"
			}
		},
		{
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#8aa9f9"
			}
		},
		{
			"scope": [
				"variable.other",
				"variable.other.local"
			],
			"settings": {
				"foreground": "#9cdbfb"
			}
		},
		{
			"scope": [
				"variable.other.global"
			],
			"settings": {
				"foreground": "#9cdbfb"
			}
		},
		{
			"scope": [
				"variable.other.readwrite",
				"variable.other.readwrite.js"
			],
			"settings": {
				"foreground": "#9cdbfb"
			}
		},
		{
			"scope": "storage.modifier.php",
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"scope": "storage.modifier.static.php",
			"settings": {
				"foreground": "#C792EA"
			}
		},
		{
			"scope": "storage.type.php",
			"settings": {
				"foreground": "#F78C6C"
			}
		},
		{
			"scope": "variable.other.php",
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"scope": [
				"keyword.operator.assignment.php",
				"constant.numeric.php"
			],
			"settings": {
				"foreground": "#F78C6C"
			}
		}
	]
}